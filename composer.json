{
  "name": "alevikzs/phmap",
  "description": "Library provide mapping some php structures (json string, array, object) to specific object. This library based on Phalcon annotations.",
  "keywords": ["json mapper", "phalcon", "annotations", "array mapper"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexey Novikov",
      "email": "alekseeey@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4.0",
    "ext-phalcon": "2.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*",
    "phalcon/devtools": "2.0.*"
  },
  "autoload": {
    "psr-4": {
      "PhMap\\": "src",
      "Tests\\": "tests"
    }
  }
}